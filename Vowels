#include <stdio.h>

int main() {
    char s[50];
    int i, count = 0;
    
    printf("Enter your string:\n");
    scanf("%s", s);
    
    for (i = 0; s[i] != '\0'; i++) {
        if (s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u' ||
            s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U') {
            count++;
        }
    }
    printf("Vowels = %d", count, s);
    return 0;
}
